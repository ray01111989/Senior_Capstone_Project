<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EDECEC"
    tools:context=".Credits">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/creditsToolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#40531C"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        android:layout_marginStart="35dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title="Credits"
        app:titleTextColor="#F57F17" />

    <ImageView
        android:id="@+id/backButtonCredits"
        android:layout_width="35dp"
        android:layout_height="56dp"
        android:background="#40531C"
        android:src="@drawable/ic_baseline_arrow_back_24"
        android:tint="#F57F17"
        android:onClick="onClickCreditsToMain"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="UseAppTint" />

    <TextView
        android:id="@+id/developers"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Devs"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.50"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias=".20"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/teamMemberGlenn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Glenn"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias=".50"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias=".27"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/teamMemberRawad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Rawad"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias=".50"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.32"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/teamMemberJared"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Jared"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias=".50"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.37"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/teamMemberYuhao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Yuhao Liu"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias=".50"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.42"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/teamMemberHenry"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Henry"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias=".50"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.47"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/sponsoredBy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/SB"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias=".50"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias=".62"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/sponsorHakan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Hakan"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias=".50"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias=".67"
        tools:ignore="HardcodedText" />

</androidx.constraintlayout.widget.ConstraintLayout>